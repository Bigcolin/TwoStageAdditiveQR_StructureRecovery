function discret_var(n)
    a = rand(n)
    z1 = a .> 0.5
    
    b = rand(n)
    z2 = b .> 0.75
    z2 += b .> 0.25
    z = [z1 z2]
end

function dgp(nSample, γ, θ, α, β)
    ϵ = randn(nSample)
    X = randn(nSample, 2)
    Z = discret_var(nSample)
    y1 = X*α + Z*β + (X*θ + Z*γ .- 1).^2 + 0.2ϵ
    y2 = X*α + Z*β + log.(1 .+ exp.(X*θ + Z*γ .- 1)) + 0.2ϵ
    X, Z, y1
end
